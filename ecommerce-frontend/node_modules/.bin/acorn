#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/amine/Documents/projects/ECOMMERCE/ecommerce-frontend/node_modules/acorn/bin/node_modules:/home/amine/Documents/projects/ECOMMERCE/ecommerce-frontend/node_modules/acorn/node_modules:/home/amine/Documents/projects/ECOMMERCE/ecommerce-frontend/node_modules:/home/amine/Documents/projects/ECOMMERCE/node_modules:/home/amine/Documents/projects/node_modules:/home/amine/Documents/node_modules:/home/amine/node_modules:/home/node_modules:/node_modules:/home/amine/Documents/projects/ECOMMERCE/ecommerce-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/amine/Documents/projects/ECOMMERCE/ecommerce-frontend/node_modules/acorn/bin/node_modules:/home/amine/Documents/projects/ECOMMERCE/ecommerce-frontend/node_modules/acorn/node_modules:/home/amine/Documents/projects/ECOMMERCE/ecommerce-frontend/node_modules:/home/amine/Documents/projects/ECOMMERCE/node_modules:/home/amine/Documents/projects/node_modules:/home/amine/Documents/node_modules:/home/amine/node_modules:/home/node_modules:/node_modules:/home/amine/Documents/projects/ECOMMERCE/ecommerce-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
